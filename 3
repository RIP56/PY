def binart_search(lis,key):
    low = 0
    high =len(lis)
    while(low<=high):
        mid = (low + high)//2
        if lis[mid]== key:
            return mid
        else:
            if lis[mid]<key:
                low = mid +1
            else:
                high= mid-1
    return -1       
lis = [1,2,3,4,5,6,7]            
res = binart_search(lis,5)
print(res)
