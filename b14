Marks ={ 'Neha' : [97,89,94, 90], 'Mitul' :[92,91,94,87], 'Shefali' : [67,99, 88,90]}
tot=0
Tot_Marks = Marks.copy()
for key, val in Marks.items():
    tot = sum(val)
    Tot_Marks[key] = tot
print(Tot_Marks)
max = 0
Topper = ' '
for key, val in Tot_Marks.items():
    if val>max:
        max = val
        Topper = key
print("Topper is : ", Topper, "with marks =" , max)
